  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "import:db-docker": "docker cp ./data/Areas.json lp5:/Areas.json; docker exec -it lp5 mongoimport --db Data --collection Locations --mode upsert --upsertFields id --jsonArray --file Areas.json; docker exec -it lp5 rm -rf Areas.json",
    "delete:db-docker": "docker exec -it lp5 bash -c \"mongosh Demo --eval 'db.Locations.deleteMany({})'\"",
    "view:db-docker": "docker exec -it lp5 bash -c \"mongosh Demo --eval 'db.Locations.find()'\""
  },
